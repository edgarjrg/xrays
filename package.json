    "standard": "^11.0.1",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "unit.js": "^2.0.0"
  },
  "dependencies": {
    "dataloader": "^1.4.0",
    "moment": "^2.22.1",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
